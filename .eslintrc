{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true,
   },
   "ecmaFeatures": {
    "es6": true,
    "classes": true,
   },
   "rules": {
     "comma-dangle": [1, "always-multiline"],
     "max-len": [1, 180, 4],
     "arrow-body-style": [0],
     "space-before-function-paren": 0,
     "consistent-return": 0,
     "func-names": 0,
     "arrow-parens": 0,
     "no-underscore-dangle": 0,
     "no-plusplus": 0
   },
}
